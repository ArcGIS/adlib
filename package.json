{
  "name": "adlib",
  "version": "0.0.1",
  "description": "Templating for deep JSON object graphs",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && concurrently \"watch 'npm run build' lib\" \"mocha -w\"",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "build": "rollup -c profiles/dev.js",
    "build:release": "rollup -c profiles/dev.js && rollup -c profiles/prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcGIS/adlib.git"
  },
  "keywords": [
    "arcgis",
    "json",
    "templating"
  ],
  "author": "Dave Bouwman <dbouwman@esri.com>, Andrew Turner <aturner@esri.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ArcGIS/adlib/issues"
  },
  "homepage": "https://arcgis.github.io/adlib",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "del": "^2.2.2",
    "eslint": "^3.19.0",
    "eslint-config-semistandard": "~7.0.0",
    "eslint-config-standard": "~6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "lodash.assignin": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isdate": "^4.0.1",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isregexp": "^4.0.1",
    "lodash.isstring": "^4.0.1",
    "lodash.map": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "mocha": "^3.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-uglify": "^1.0.1",
    "watch": "^1.0.2"
  }
}
